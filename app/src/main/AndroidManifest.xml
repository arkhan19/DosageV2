<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="healerkart.com.dosage" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Alpha.AddDosage"
            android:label="New Dosage"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".Alpha.Home"
            android:label="New Dosage" />


        <receiver android:name="healerkart.com.dosage.Delta.AlarmSetter">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <receiver android:name="healerkart.com.dosage.Delta.AlarmReceiver"></receiver>

        <service android:name="healerkart.com.dosage.Delta.AlarmService"></service>





    </application>

</manifest>
